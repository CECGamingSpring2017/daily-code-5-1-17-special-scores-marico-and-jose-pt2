#include<iostream>
#include <cstdlib> 
#include <ctime> 
using namespace std;
int main() {
	int SPECIAL[7];
	srand(static_cast<unsigned int>(time(0)));
	for (int i = 0; i < 7; i++) {
		int number = rand() % 11;
		SPECIAL[i] = number;
	}
	
	cout << "your strength" << SPECIAL[0]<< endl;
	cout << "your Perception " << SPECIAL[1] << endl;
	cout << "your Endurance" << SPECIAL[2] << endl;
	cout << "your Charisma" << SPECIAL[3] << endl;
	cout << "your Intelligence" << SPECIAL[4] << endl;
	cout << "your Agility" << SPECIAL[5] << endl;
	cout << "your Luck " << SPECIAL[6] << endl;
	

}
